<%- include('layout/header') %>

<div class="container m-4 row row-cols-3 g-1 justify-content-center">
   <% users.forEach(el => { %>
      <div class="card m-3" style="width: 18rem;">
        <img src="<%= el.image %>" class="card-img-top img-thumbnail" alt="...">
        <div class="card-body">
          <h5 class="card-title">Name: <%= el.name %>
          </h5>
          <h5 class="card-title">Standard: <%= el.standard %>
          </h5>
          <br>
          <a href="/more/<%= el._id %>" class="btn btn-primary">View</a>
          
        </div>
      </div>
      <% }) %>
  </div>











<%- include('layout/footer') %>
   